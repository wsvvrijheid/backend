{
  "kind": "collectionType",
  "collectionName": "hashtag_posts",
  "info": {
    "name": "HashtagPost",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "text": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "maxLength": 30,
      "required": true,
      "targetField": "text"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": ["images"],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "hashtag": {
      "via": "posts",
      "model": "hashtag"
    }
  }
}
